{
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "parcel": "parcel example/index.html"
  },
  "peerDependencies": {
    "axios": ">=0.24"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@webinex/flippo",
  "author": "Webinex Dev",
  "module": "dist/flippo.esm.js",
  "size-limit": [
    {
      "path": "dist/flippo.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/flippo.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "axios": "0.24",
    "husky": "^7.0.4",
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^6.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
